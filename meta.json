{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "message": "项目名称"
    },
    "description": {
      "type": "string",
      "required": false,
      "message": "项目描述",
      "default": "A vue-parster-tpl project"
    },
    "author": {
      "type": "string",
      "message": "作者"
    },
    "vuex": {
      "type": "list",
      "message": "状态管理机(vuex)",
      "choices": [
        "yes",
        "no"
      ]
    }
  },
  "filters": {
    "src/store/*.js":"vuex === 'yes'",
    "src/pages/index/**/*.*":"vuex === 'yes'",
    "src/pages/demo/**/*.*":"vuex === 'no'"
  },
  "skipInterpolation": "src/**/*.vue"
}