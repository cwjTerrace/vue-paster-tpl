{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "message": "项目名称"
    },
    "description": {
      "type": "string",
      "required": false,
      "message": "项目描述",
      "default": "A vue-parster-tpl project"
    },
    "author": {
      "type": "string",
      "message": "作者"
    },
    "vuex": {
      "type": "list",
      "choices": [
        "yes",
        "no"
      ]
    }
  },
  "filters": {
    "src/store/*.js":"vuex === 'yes'"
  }
}